<% if @canonical_url %>
<% content_for :meta do %>
<link href="<%= @canonical_url %>" rel="canonical">
<% end %>
<% end %>

<% content_for :content do %>
  <div>
    <div class="overflow-hidden">
      <%= load_pack("discover") %>
      <%= react_component "Discover", props: @react_discover_props, prerender: true %>
    </div>
  </div>
  <%= render "home/shared/footer" %>
<% end %>
